{
  "name": "tpp-excel",
  "private": true,
  "version": "0.0.1",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "sideload": "office-addin-debugging start manifest.xml --app excel",
    "stop": "office-addin-debugging stop manifest.xml",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "validate": "office-addin-manifest validate manifest.xml",
    "generate:metadata": "custom-functions-metadata generate ./functions/functions.ts ./functions/functions.json",
    "install-certs": "office-addin-dev-certs install"
  },
  "dependencies": {
    "@tanstack/vue-table": "^8.5.11",
    "@vicons/fluent": "^0.12.0",
    "lodash-es": "^4.17.21",
    "pinia": "^2.0.21",
    "vue": "^3.2.37",
    "vue-router": "^4.1.5"
  },
  "devDependencies": {
    "@types/custom-functions-runtime": "^1.6.5",
    "@types/office-js": "^1.0.272",
    "@typescript-eslint/eslint-plugin": "^5.35.1",
    "@typescript-eslint/parser": "^5.35.1",
    "@vitejs/plugin-vue": "^3.0.3",
    "@vitejs/plugin-vue-jsx": "^2.0.0",
    "custom-functions-metadata": "^1.4.0",
    "eslint": "^8.23.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-vue": "^9.4.0",
    "naive-ui": "^2.33.1",
    "office-addin-debugging": "^4.5.0",
    "office-addin-dev-settings": "^1.13.0",
    "office-addin-manifest": "^1.10.0",
    "prettier": "^2.7.1",
    "typescript": "^4.8.2",
    "vfonts": "^0.0.3",
    "vite": "^3.0.9",
    "vite-plugin-office-addin": "^1.0.0",
    "vite-plugin-windicss": "^1.8.7",
    "vitest": "^0.22.1",
    "vue-eslint-parser": "^9.0.3",
    "vue-tsc": "^0.40.4",
    "windicss": "^3.5.6"
  },
  "prettier": {
    "semi": false,
    "printWidth": 100,
    "singleAttributePerLine": true
  }
}
