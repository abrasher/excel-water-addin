{
  "name": "tpp-excel",
  "private": true,
  "version": "0.0.1",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "sideload": "office-addin-debugging start manifest.xml --app excel",
    "stop": "office-addin-debugging stop manifest.xml",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "validate": "office-addin-manifest validate manifest.xml",
    "generate:metadata": "custom-functions-metadata generate ./functions/functions.ts ./functions/functions.json"
  },
  "dependencies": {
    "@vicons/fluent": "^0.12.0",
    "lodash-es": "^4.17.21",
    "pinia": "^2.0.14",
    "vue": "^3.2.25",
    "vue-router": "^4.0.16"
  },
  "devDependencies": {
    "@types/custom-functions-runtime": "^1.6.4",
    "@types/office-js": "^1.0.258",
    "@typescript-eslint/eslint-plugin": "^5.29.0",
    "@typescript-eslint/parser": "^5.29.0",
    "@vitejs/plugin-vue": "^2.3.3",
    "@volar-plugins/prettyhtml": "^1.0.0",
    "custom-functions-metadata": "^1.2.9",
    "eslint": "^8.18.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-vue": "^9.1.1",
    "naive-ui": "^2.29.0",
    "office-addin-debugging": "^4.3.14",
    "office-addin-dev-settings": "^1.11.4",
    "office-addin-manifest": "^1.8.1",
    "prettier": "^2.7.1",
    "typescript": "^4.7.4",
    "vfonts": "^0.0.3",
    "vite": "^2.9.9",
    "vite-plugin-windicss": "^1.8.6",
    "vitest": "^0.14.1",
    "vue-eslint-parser": "^9.0.2",
    "vue-tsc": "^0.34.7",
    "windicss": "^3.5.6"
  },
  "prettier": {
    "semi": false,
    "printWidth": 120
  }
}
