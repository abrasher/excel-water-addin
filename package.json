{
  "name": "tpp-excel",
  "private": true,
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "sideload": "office-addin-debugging start manifest.xml --app excel",
    "stop": "office-addin-debugging stop manifest.xml",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "validate": "office-addin-manifest validate manifest.xml",
    "generate:metadata": "custom-functions-metadata generate ./functions/functions.ts ./functions/functions.json",
    "install-certs": "office-addin-dev-certs install",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "trace": "vue-tsc --noEmit --generateTrace trace --incremental false"
  },
  "dependencies": {
    "@tanstack/vue-table": "^8.21.2",
    "@vicons/fluent": "^0.13.0",
    "lodash-es": "^4.17.21",
    "markdown-it-mathjax3": "^4.3.2",
    "pinia": "^3.0.1",
    "vue": "^3.5.13",
    "vue-router": "^4.5.0"
  },
  "devDependencies": {
    "@types/custom-functions-runtime": "^1.6.8",
    "@types/office-js": "^1.0.484",
    "@typescript-eslint/eslint-plugin": "^8.28.0",
    "@typescript-eslint/parser": "^8.28.0",
    "@vitejs/plugin-vue": "^5.2.3",
    "@vitejs/plugin-vue-jsx": "^4.1.2",
    "@vue/typescript": "^1.8.20",
    "custom-functions-metadata": "^2.1.3",
    "eslint": "^9.23.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-vue": "^10.0.0",
    "naive-ui": "^2.41.0",
    "office-addin-debugging": "^6.0.3",
    "office-addin-dev-settings": "^3.0.3",
    "office-addin-manifest": "^2.0.3",
    "prettier": "^3.5.3",
    "prettier-plugin-organize-imports": "^4.1.0",
    "typescript": "^5.8.2",
    "vfonts": "^0.0.3",
    "vite": "^6.2.3",
    "vite-plugin-ssr": "^0.4.142",
    "vite-plugin-windicss": "^1.9.4",
    "vitepress": "^1.6.3",
    "vitepress-plugin-mermaid": "^2.0.17",
    "vitest": "^3.0.9",
    "vue-eslint-parser": "^10.1.1",
    "vue-tsc": "^2.2.8",
    "windicss": "^3.5.6"
  },
  "prettier": {
    "semi": false,
    "printWidth": 100,
    "singleAttributePerLine": true
  }
}
